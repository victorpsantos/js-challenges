{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "plugins": ["prettier", "import"],
  "extends": [
    "eslint:recommended",
    "plugin:prettier",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "prettier/prettier": "error",
    "import/order": "error"
  }
}
